{#
/**
 * @file
 * Display Coordinator name as a link to Community Persona page
 */
#}

  <div>
  {% for item in items %}
    <div>
      <a class="btn btn-primary" href='{{ item.content['#url'].uri }}'>
        {% if not (item.content['#title'] starts with 'http') %}
          {{item.content['#title']}}
        {% else %}
          Read More
        {% endif %}
      </a>
    </div>
  {% endfor %}
</div>
